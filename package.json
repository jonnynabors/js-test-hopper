{
  "name": "test-hopper",
  "displayName": "test-hopper",
  "description": "Quickly hop from your implementation to your tests and vice-versa",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.42.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jonnynabors/test-hopper.git"
  },
  "publisher": "jonnynabors",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.testHopper"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.testHopper",
        "title": "Hop to test or implementation"
      }
    ],
    "keybindings": [
      {
        "command": "extension.testHopper",
        "key": "ctrl+alt+t",
        "mac": "ctrl+cmd+t"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.42.0",
    "glob": "^7.1.5",
    "mocha": "^6.2.2",
    "typescript": "^3.6.4",
    "tslint": "^5.20.0",
    "vscode-test": "^1.2.2"
  }
}
